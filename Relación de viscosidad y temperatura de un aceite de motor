# Datos 
temperaturas = [0, 20, 40, 60, 80, 100, 120]
viscosidad = [150, 120, 90, 70, 50, 40, 30]

plt.figure(figsize=(10, 6))
plt.plot(temperaturas, viscosidad, marker='o', linestyle='--', color='red')
plt.title('Relación de Viscosidad y Temperatura de un Aceite de Motor')
plt.xlabel('Temperatura (°C)')
plt.ylabel('Viscosidad (cP)')
plt.grid()
plt.show()
